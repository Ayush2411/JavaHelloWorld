pipeline {
  agent any
   stages {
      stage('checkout from develop') {
         steps {
           sh label: '', 
           script: 
           '''
           git pull origin master
           '''
         }
      }
      stage('build') {
         steps {
           sh 'mvn clean package'
         }
      }
      stage('checkout other repo') {
         steps {
           sh label: '', 
           script: 
           '''
           cd /home/aditya/aditya/personal/git
           git pull origin master
           cp /var/lib/jenkins/workspace/Hello-world-Java/target/demo1-0.0.1-SNAPSHOT.jar /home/aditya/aditya/personal/git/Gittest
           '''
         }
      }
   }
}
