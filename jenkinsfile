pipeline {
    agent any
    stages {
        stage('Set Up') {
            steps {
                sh 'date'
            }
        }
        stage('Build') { 
            steps {
                sh 'mvn clean package' 
            }
       }
       stage('Run') { 
            steps {
                sh 'cd /target'
                sh 'nohup java -jar *jar'
            }
       }
    }
}
