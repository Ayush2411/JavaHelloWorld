pipeline {
    agent any
    stages {
        stage('Set Up') {
            steps {
                sh 'date'
            }
        }
        stage('Build') { 
            steps {
                sh 'mvn clean package' 
            }
       }
       stage('Run') { 
            steps {
                sh 'cd /var/lib/jenkins/workspace/testPipeline1/target/'
                sh 'nohup java -jar *.jar &'
            }
       }
    }
}
